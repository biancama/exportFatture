<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="adempiere">
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
        <plugin type="org.mybatis.generator.plugins.FluentBuilderMethodsPlugin" />
        <jdbcConnection driverClass="org.postgresql.Driver" connectionURL="jdbc:postgresql://localhost:5432/adempiere" userId="adempiere" password="adempiere"/>

        <javaModelGenerator targetPackage="com.biancama.meoni.domain.adempiere" targetProject="MAVEN"/>
        <sqlMapGenerator targetPackage="com.biancama.meoni.domain.adempiere" targetProject="MAVEN" />
        <table tableName="%">
            <property name="useActualColumnNames" value="true"/>
        </table>
    </context>
</generatorConfiguration>